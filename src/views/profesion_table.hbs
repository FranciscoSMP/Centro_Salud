<h2>Lista de Profesiones</h2>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Nombre de la Profesion</th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
        {{#each profesion}}
        <tr>
            <td>{{this.Id_Profesion}}</td> <!-- AsegÃºrate de que los campos coincidan con la base de datos -->
            <td>{{this.Profesion_Oficio}}</td>
            <td>
                <a href="/profesion/editar/{{this.Id_Profesion}}">Editar</a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
