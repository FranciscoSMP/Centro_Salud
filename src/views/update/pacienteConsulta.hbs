<h2 class="text-center my-4">Actualizar Paciente - Consulta</h2>
<form action="/paciente_consulta/actualizar" method="POST" class="container form-General">
    <input type="hidden" name="Paciente_Consulta" value="{{pacienteConsulta.Paciente_Consulta}}" />
    <div class="form-group mb-3">
        <label for="DPI_Paciente" class="form-label">DPI del paciente</label>
        <input type="text" name="DPI_Paciente" id="DPI_Paciente" class="form-control" value="{{pacienteConsulta.DPI_Paciente}}" required>
        <label for="Id_Consulta" class="form-label">ID de la consulta</label>
        <input type="text" name="Id_Consulta" id="Id_Consulta" class="form-control" value="{{pacienteConsulta.Id_Consulta}}" required>
    </div>
    <div class="text-center">
    <button type="submit" class="btn btn-primary">Actualizar</button>
    </div>
</form> 
